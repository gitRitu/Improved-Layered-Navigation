<?xml version="1.0"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Dotsquares_Base::css/config.css"/>
    </head>
    <body>
        <referenceContainer name="messages.wrapper">
            <block class="Dotsquares\Base\Block\Adminhtml\Messages" name="dsbase.messages" template="Dotsquares_Base::messages.phtml" after="page.messages"/>
        </referenceContainer>
        <referenceContainer name="left">
            <block class="Dotsquares\Base\Block\Adminhtml\Notification" name="dsbase.notification" template="Dotsquares_Base::notification.phtml"/>
        </referenceContainer>
    </body>
</page>
